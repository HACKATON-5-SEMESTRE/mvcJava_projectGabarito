@Controller
@RequestMapping("/aluno")
public class AlunoNotasController {

    @GetMapping("/notas")
    public String verNotas(Model model) {
        // buscar notas do aluno autenticado
        return "aluno/notas";
    }

    @GetMapping("/feedbacks")
    public String verFeedbacks(Model model) {
        // buscar feedbacks
        return "aluno/feedbacks";
    }
}
